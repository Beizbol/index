{
	"New+ HTML": {
		"prefix": "!!",
		"body": [
			"<!DOCTYPE html>",
			"<html lang=\"en\">",
			"    <head>",
			"        <meta charset=\"UTF-8\">",
			"        <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">",
			"        <title>Title</title>",
			"        <link rel=\"stylesheet\" href=\"style.css\">",
			"        <script type=\"module\" src=\"module.js\" defer>",
			"            import { name, run } from \"./module.js\";",
			"            console.log(\"running\", name);",
			"            run();",
			"        </script>",
			"        <style>",
			"            html {",
			"                color-scheme: light dark;",
			"            }",
			"        </style>",
			"        <script>",
			"            const cs = localStorage.getItem(\"colorScheme\");",
			"            if (!cs) return;",
			"            const css_root = document.rootElement.style;",
			"            if (cs === \"dark\") {",
			"                css_root.colorScheme = \"dark\";",
			"                return;",
			"            }",
			"            if (cs === \"light\") {",
			"                css_root.colorScheme = \"light\";",
			"                return;",
			"            }",
			"        </script>",
			"    </head>",
			"    <body>",
			"        <header><nav><a href=\"/\">Home</a></nav></header>",
			"        <main>",
			"            <h1>Title</h1>",
			"        </main>",
			"        <footer>Copyright &copy; 2025.</footer>",
			"    </body>",
			"</html>"
		],
		"description": "New & Improved HTML File."
	}
}
